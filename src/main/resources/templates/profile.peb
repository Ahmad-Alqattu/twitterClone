    {% extends "base.peb" %}
        {% block title %}Profile{% endblock %}
    {% block content %}
<h1>{{ user.username }}</h1>
<div id="tweets">
    {% for tweet in tweets %}
    <div class="card mt-3">
        <div class="card-body">
            <p class="card-text">{{ tweet.content }}</p>
            <p class="card-text"><small class="text-muted">{{ tweet.timestamp }}</small></p>
        </div>
    </div>
    {% endfor %}
</div>
{% endblock %}
